*filter
:INPUT ACCEPT [0:0]
:FORWARD ACCEPT [0:0]
:OUTPUT ACCEPT [0:0]
:basesystem_icmp - [0:0]
:basesystem_ssh - [0:0]
:bigbluebutton - [0:0]
:prometheus - [0:0]
-A INPUT -i lo -j ACCEPT
-A INPUT -p tcp -m tcp --tcp-flags ACK ACK -j ACCEPT
-A INPUT -p tcp -m tcp ! --tcp-flags FIN,SYN,RST,ACK SYN -m state --state NEW -j DROP
-A INPUT -p tcp -m tcp --tcp-flags FIN,SYN,RST,PSH,ACK,URG FIN,SYN,RST,PSH,ACK,URG -j DROP
-A INPUT -p tcp -m tcp --tcp-flags FIN,SYN,RST,PSH,ACK,URG NONE -j DROP
-A INPUT -p tcp -m tcp --tcp-flags RST RST -m limit --limit 2/sec --limit-burst 2 -j ACCEPT
-A INPUT -p tcp -m tcp --tcp-flags RST RST -j DROP
-A INPUT -m state --state RELATED,ESTABLISHED -j ACCEPT
-A INPUT -p icmp -j basesystem_icmp
-A INPUT -p tcp -m tcp -s 94.246.169.68/32 --dport 22 -j basesystem_ssh
-A INPUT -p tcp -m tcp -s 94.246.169.68/32 --dport 9100 -j prometheus
-A INPUT -p tcp -m tcp -s 94.246.169.69/32 --dport 22 -j basesystem_ssh
-A INPUT -p tcp -m tcp -s 94.246.169.69/32 --dport 9100 -j prometheus
-A INPUT -m udp -p udp --dport 16384:32768 -j bigbluebutton
-A INPUT -m tcp -p tcp --dport 80 -j bigbluebutton
-A INPUT -m tcp -p tcp --dport 443 -j bigbluebutton
-A INPUT -j DROP
-A basesystem_icmp -j ACCEPT
-A basesystem_ssh -j ACCEPT
-A bigbluebutton -j ACCEPT
-A prometheus -j ACCEPT
COMMIT
